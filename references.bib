@article{eduardoTrophicEcologyHabitat2020,
  title = {Trophic Ecology, Habitat, and Migratory Behaviour of the Viperfish {{Chauliodus}} Sloani Reveal a Key Mesopelagic Player},
  author = {Eduardo, Leandro Nol{\'e} and {Lucena-Fr{\'e}dou}, Fl{\'a}via and Mincarone, Michael Maia and Soares, Andrey and Le Loc'h, Fran{\c c}ois and Fr{\'e}dou, Thierry and M{\'e}nard, Fr{\'e}d{\'e}ric and Bertrand, Arnaud},
