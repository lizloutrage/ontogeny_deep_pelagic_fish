@article{flynnCrossbasinHeterogeneityLanternfish2012,
  title = {Cross-Basin Heterogeneity in Lanternfish (Family {{Myctophidae}}) Assemblages and Isotopic Niches ({{$\delta$13C}} and {{$\delta$15N}}) in the Southern {{Tasman Sea}} Abyssal Basin},
