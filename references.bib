@article{evseenkoMicronektonIchthyoplanktonFrontal2006,
  title = {On Micronekton and Ichthyoplankton of the Frontal Zone of the Gulf Stream and Subpolar Waters ({{Polygone}} ``{{Titanic}}'')},
