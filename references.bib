@article{menardStableIsotopePatterns2014,
  title = {Stable Isotope Patterns in Micronekton from the {{Mozambique Channel}}},
  author = {M{\'e}nard, Fr{\'e}d{\'e}ric and Benivary, Hermann Doris and Bodin, Nathalie and Coffineau, Nathalie and Le Loc'h, Fran{\c c}ois and Mison, Thomas and Richard, Pierre and Potier, Michel},
  year = {2014},
  month = feb,
  journal = {Deep Sea Research Part II: Topical Studies in Oceanography},
  volume = {100},
  pages = {153--163},
  issn = {09670645},
  doi = {10.1016/j.dsr2.2013.10.023},
  abstract = {We measured the stable carbon ({$\delta$}13C) and nitrogen ({$\delta$}15N) isotopic composition of tissues of micronektonic organisms (fishes, squids, crustaceans and gelatinous organisms) collected in the Mozambique Channel during two scientific cruises in 2008 and 2009. The oceanic circulation in the Mozambique Channel is dominated by mesoscale cyclonic and anticyclonic eddies which play a key role in biological processes of less-productive deep-sea ecosystems. We investigated the potential impact of mesoscale features on the {$\delta$}13C and {$\delta$}15N values of 32 taxa of micronekton. Fishes, squids, crustaceans and gelatinous organisms encompassed a wide range of isotopic niches, with large overlaps among species. Our results showed that mesoscale features did not really influence the isotopic signatures of the sampled organisms, although cyclonic eddies can occasionally impact the nitrogen signatures of micronekton. We show that {$\delta$}13C values were intermediate between standard offshore and nearshore signatures, suggesting that pelagic production in the Mozambique Channel could be partly supported by the transport and export of inorganic and organic particles from the Mozambican coast toward the offshore area. Trophic levels calculated from {$\delta$}15N values ranged from 2.6 to 4.2, showing that micronekton taxa can be tertiary consumers in the Mozambique Channel. Our findings evidenced clusters of micronektonic organisms according to their {$\delta$}15N or {$\delta$}13C isotopic signatures, but variations in stable isotope values reflect a complex set of embedded processes linked to physical mesoscale dynamics (rotational dynamics of eddies) and basic biology and ecology of micronektonic organisms (vertical habitat, migration pattern, dietary habits, body length) that are discussed with regard to the stable isotope method based on time-integrated assimilated food.},
  langid = {english},
  file = {C\:\\Users\\lloutr01\\Zotero\\storage\\QQW3RGIE\\Ménard et al. - 2014 - Stable isotope patterns in micronekton from the Mo.pdf}
}

@article{mertenIntegrativeAssessmentCombining2021,
  title = {An {{Integrative Assessment Combining Deep-Sea Net Sampling}}, in Situ {{Observations}} and {{Environmental DNA Analysis Identifies Cabo Verde}} as a {{Cephalopod Biodiversity Hotspot}} in the {{Atlantic Ocean}}},
  author = {Merten, V{\'e}ronique and Bayer, Till and Reusch, Thorsten B. H. and Puebla, Oscar and Fuss, Janina and Stefanschitz, Julia and Lischka, Alexandra and Hauss, Helena and Neitzel, Philipp and Piatkowski, Uwe and Czudaj, Stephanie and Christiansen, Bernd and Denda, Anneke and Hoving, Henk-Jan T.},
  year = {2021},
  month = nov,
  journal = {Frontiers in Marine Science},
  volume = {8},
  pages = {760108},
  issn = {2296-7745},
  doi = {10.3389/fmars.2021.760108},
  abstract = {The deep sea is among the largest, most biologically diverse, yet least-explored ecosystems on Earth. Baseline information on deep-sea biodiversity is crucial for understanding ecosystem functioning and for detecting community changes. Here, we established a baseline of cephalopod community composition and distribution off Cabo Verde, an archipelago in the eastern tropical Atlantic. This baseline served to test the hypothesis that Cabo Verde is biogeographically separated from other Macaronesian archipelagos and allowed the identification of cephalopod species which may play a role in the Macaronesian carbon cycle and oceanic food web. To investigate cephalopod community composition, this study used 746 individual cephalopods obtained by nets (0\textendash 1000 m) and 52 cephalopod encounters during video surveys with either towed camera (0\textendash 2500 m) or manned submersible (0\textendash 375 m). Additionally, environmental DNA (eDNA) metabarcoding on 105 seawater samples (50\textendash 2500 m), using an 18S rRNA universal cephalopod primer pair, and a species-specific primer pair for               Taningia danae               resulted in the detection of 32 cephalopod taxa. When combined, the three methods detected a total of 87 taxa, including 47 distinct species. Each method contributed between 7 and 54\% of taxa that were not detected by the other methods, indicating that multiple methodological approaches are needed for optimal deep-sea cephalopod biodiversity assessments. This study documents the occurrences of six species and three genera for the first time in waters surrounding Cabo Verde. Video surveys and eDNA analysis detected               Taningia danae               recurrently (100\textendash 2500 m). eDNA metabarcoding proved to be a powerful tool for cephalopod biodiversity monitoring and complementary to traditional sampling methods. When also including literature records, Cabo Verde hosts at least 102 cephalopod taxa including 30 families and 64 benthic and pelagic species. The total number and species composition of Cabo Verde cephalopods is similar to the Canary Islands and Azores, two known cephalopod biodiversity hotspots, but the Cabo Verde octopus fauna seems to differ. Due to a range of life history characteristics, we hypothesize that the squids               Taningia danae               (Octopoteuthidae) and               Sthenoteuthis pteropus               (Ommastrephidae) are important in the carbon cycle of Macaronesia. As a cephalopod biodiversity hotspot Cabo Verde could function as a model region to investigate cephalopod biology and ecology in a rapidly changing Atlantic Ocean.},
  langid = {english},
  file = {C\:\\Users\\lloutr01\\Zotero\\storage\\GW9BS7RN\\Merten et al. - 2021 - An Integrative Assessment Combining Deep-Sea Net S.pdf}
}

@article{mevelRideSnakePit1989,
  title = {La Ride Du {{Snake Pit}} (Dorsale M\'edio-{{Atlantique}}, 23\textdegree 22'{{N}}) : R\'esultats Pr\'eliminaires de La Campagne {{HYDROSNAKE}}},
  author = {Mevel, Catherine and Auzende, Jean-Marie and Cannat, Mathilde and Donval, Jean-Pierre and Dubois, Jacques and Fouquet, Yves and Gente, Pascal and Grimaud, Daniel and Karson, Jeffrey A. and Segonzac, Michel and Stievenard, Michel},
  year = {1989},
  journal = {Comptes Rendus de l'Academie des Sciences Serie II},
  volume = {308},
  number = {6},
  pages = {545--552},
  issn = {1251-8069},
  abstract = {Un des objectifs de la campagne HYDROSNAKE (12/6-14/7/88) \'etait la reconnaissance in situ, \`a partir du submersible Nautile, de la ride n\'eovoLcanique du Snake Pit, \`a 23\textdegree 22'N sur la dorsale m\'edio-Atlantique et l'\'etude des manifestations hydrothermales \`associ\'ees. Dix plong\'ees ont \'et\'e r\'ealis\'ees sur ce site et ont eu pour objet des observations et des pr\'el\`evements g\'eologiques et biologiques, des pr\'el\`evements de fluides hydrothermaux et des mesures gravim\'etriques. Les r\'esultats pr\'eliminaires obtenus \`a bord ont permis l'\'etablissement d'une carte g\'eologique au 1/5000 de la zone et de conclure que la ride du Snake Pit est form\'ee d'un empilement de coul\'ees de laves en coussins et en tubes, structur\'e dans sa partie axiale par des failles et des fissures. La localisation des sites hydrothermaux est clairement guid\'ee par ces accidents. One of the goals of the HYDROSNAKE cruise with the submersible Nautile was the exploration of the neovolcanic ridge of the Snake Pit, in the mid-Atlantic Ridge, 23\textdegree 22' N, and the study of the associated hydrothermal vents. During 10 dives at this site, geological observations, sulfides, basalts and fluids sampling, gravimetric measurements as weil as biological specimen collection were performed. A geological map of the are a was made at a 1/5,000 scale. It shows that the Snake Pit ridge is a constructional feature made up of pillow lavas and lava tubes, which was subsequently structured by faulting. Location of the hydrothermal sites is clearly dependant on tectonic accidents.}
}

