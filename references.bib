@article{romero-romeroBodySizebasedTrophic2016,
  title = {Body Size-Based Trophic Structure of a Deep Marine Ecosystem: {{A Size-Based Trophic Structure}}},
  shorttitle = {Body Size-Based Trophic Structure of a Deep Marine Ecosystem},
  author = {{Romero-Romero}, Sonia and {Molina-Ram{\'i}rez}, Axayacatl and H{\"o}fer, Juan and Acu{\~n}a, Jos{\'e} Luis},
  year = {2016},
  month = jan,
  journal = {Ecology},
  volume = {97},
  number = {1},
  pages = {171--181},
  issn = {00129658},
  doi = {10.1890/15-0234.1},
  abstract = {Nitrogen stable isotope ratios ({$\delta$}15N) and body size were used to describe the size-based trophic structure of a deep-sea ecosystem, the Avil\'es submarine Canyon (Cantabrian Sea, Southern Bay of Biscay). We analyzed {$\delta$}15N of specimens collected on a seasonal basis (March 2012, October 2012, and May 2013), from a variety of zones (benthic, pelagic), taxa (from zooplankton through invertebrates and fishes to giant squids and cetaceans), or depths (from surface to 4700 m) that spanned nine orders of magnitude in body mass. Our data reveal a strong linear dependence of trophic level on body size when data were considered either individually, aggregated into taxonomical categories, or binned into size classes. The three approaches render similar results that were not significantly different and yielded predator:prey body mass ratios (PPMR) of 1156:1, 3792:1 and 2718:1, respectively. Thus, our data represent unequivocal evidence of interspecific, size-based trophic structure of a whole ecosystem based on taxonomic/functional categories. We studied the variability in {$\delta$}15N not explained by body mass (W) using linear mixed modeling and found that the {$\delta$}15N vs. log10 W relationship holds for both pelagic and benthic systems, with benthic organisms isotopically enriched relative to pelagic organisms of the same size. However there is a marked seasonal variation potentially related to the recycling state of the system.},
  langid = {english},
  file = {C\:\\Users\\lloutr01\\Zotero\\storage\\QUQVRW9U\\Romero-Romero et al. - 2016 - Body size-based trophic structure of a deep marine.pdf}
}

@article{romero-romeroDifferencesTrophicEcology2019,
  title = {Differences in the Trophic Ecology of Micronekton Driven by Diel Vertical Migration},
  author = {Romero-Romero, Sonia and Choy, C. Anela and Hannides, Cecelia C. S. and Popp, Brian N. and Drazen, Jeffrey C.},
  year = {2019},
  month = jul,
  journal = {Limnology and Oceanography},
  volume = {64},
  number = {4},
  pages = {1473--1483},
  issn = {0024-3590, 1939-5590},
  doi = {10.1002/lno.11128},
  abstract = {Many species of micronekton perform diel vertical migrations (DVMs), which ultimately contributes to carbon export to the deep sea. However, not all micronekton species perform DVM, and the nonmigrators, which are often understudied, have different energetic requirements that might be reflected in their trophic ecology. We analyze bulk tissue and whole animal stable nitrogen isotopic compositions ({$\delta$}15N values) of micronekton species collected seasonally between 0 and 1250 m depth to explore differences in the trophic ecology of vertically migrating and nonmigrating micronekton in the central North Pacific. Nonmigrating species exhibit depth-related increases in {$\delta$}15N values mirroring their main prey, zooplankton. Higher variance in {$\delta$}15N values of bathypelagic species points to the increasing reliance of deeper dwelling micronekton on microbially reworked, very small suspended particles. Migrators have higher {$\delta$}15N values than nonmigrators inhabiting the epipelagic zone, suggesting the consumption of material during the day at depth, not only at night when they migrate closer to the surface. Migrating species also appear to eat larger prey and exhibit a higher range of variation in {$\delta$}15N values seasonally than nonmigrators, likely because of their higher energy needs. The dependence on material at depth enriched in 15N relative to surface particles is higher in migratory fish that ascend only to the lower epipelagic zone. Our results confirm that stark differences in the food habits and dietary sources of micronekton species are driven by vertical migrations.},
  langid = {english},
  file = {C\:\\Users\\lloutr01\\Zotero\\storage\\2KUE9X9Y\\A new probabilistic method for quantifying n-dimensional.pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\2VMP3YJ2\\New multidimensional functionaldiversity indices for a multifaceted framework in functional ecology. Ecology89.pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\3LMZZ3UQ\\Romero‐Romero et al. - 2019 - Differences in the trophic ecology of micronekton .pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\3XJFFMJ4\\An Empirical Evaluation of the Utility of Convex Hull and.pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\8WQXUJVM\\CAN STABLE ISOTOPE RATIOS PROVIDE FOR COMMUNITY-WIDE.pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\9V9K64HX\\Lipid correction for carbon stable isotope analysis of deep-sea fishes.pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\AIFICPTZ\\Stable Isotope Trajectory Analysis (SITA) A new approach.pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\AKJEVGHC\\Quantifying the multiple facets of isotopic diversity.pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\AWPNB7BK\\The functional ecology of mangrove fishes across the Isthmus of Panama.pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\CABVYK5X\\Isotopic Diversity Indices How Sensitive to Food Web.pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\DZET3NCA\\A_guide_to_the_use_of_tRophicPosition_2016-06-12 (1).pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\FKMQ697D\\Comparing isotopic niche widths among and within.pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\GE9JVPWJ\\Getting to the fat of the matter models, methods.pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\GKCGHEPE\\Individual growth as a non‐dietary determinant of the isotopic niche metrics.pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\GQ4ZYWAK\\tRophicPosition  an r package for the Bayesian estimation of trophic.pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\HVG2KA9Z\\Using stable isotopes to estimate trophic position models, methods.pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\IRTKNG67\\Trophic niche overlap between sympatric harbour seals.pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\LXBSAWEP\\Food web structure in relation to environmental drivers across a continental.pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\UZE3GA9E\\Spatio-temporal patterns in stable isotope composition.pdf;C\:\\Users\\lloutr01\\Zotero\\storage\\ZYSPNMGL\\High intraspecific variability in the functional niche of a.pdf}
