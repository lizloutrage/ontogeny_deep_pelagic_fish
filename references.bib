@article{klevjerLargeScalePatterns2016a,
  title = {Large Scale Patterns in Vertical Distribution and Behaviour of Mesopelagic Scattering Layers},
  author = {Klevjer, T. A. and Irigoien, X. and R{\o}stad, A. and {Fraile-Nuez}, E. and {Ben{\'i}tez-Barrios}, V. M. and Kaartvedt., S.},
