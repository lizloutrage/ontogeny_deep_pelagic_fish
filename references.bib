  shorttitle = {Using Large Scale Surveys to Investigate Seasonal Variations in Seabird Distribution and Abundance. {{Part II}}},
  author = {Pettex, Emeline and Laran, Sophie and Authier, Matthieu and Blanck, Aur{\'e}lie and Dor{\'e}mus, Ghislain and Falchetto, H{\'e}l{\`e}ne and Lambert, Charlotte and Monestiez, Pascal and St{\'e}fan, Eric and Van Canneyt, Olivier and Ridoux, Vincent},
  year = {2017},
