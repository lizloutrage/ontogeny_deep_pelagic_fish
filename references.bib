@article{annasawmyMicronektonDielMigration2018,
  title = {Micronekton Diel Migration, Community Composition and Trophic Position within Two Biogeochemical Provinces of the {{South West Indian Ocean}}: {{Insight}} from Acoustics and Stable Isotopes},
  shorttitle = {Micronekton Diel Migration, Community Composition and Trophic Position within Two Biogeochemical Provinces of the {{South West Indian Ocean}}},
  author = {Annasawmy, P. and Ternon, J.F. and Marsac, F. and Cherel, Y. and B{\'e}hagle, N. and Roudaut, G. and {Lebourges-Dhaussy}, A. and Demarcq, H. and Moloney, C.L. and Jaquemet, S. and M{\'e}nard, F.},
  year = {2018},
  month = aug,
  journal = {Deep Sea Research Part I: Oceanographic Research Papers},
  volume = {138},
  pages = {85--97},
  issn = {09670637},
  doi = {10.1016/j.dsr.2018.07.002},
  abstract = {Spatial distribution, community composition and trophic roles of micronekton (crustaceans, fishes and squids) were investigated in the Indian South Subtropical Gyre (ISSG) province and the East African Coastal province (EAFR), by combining acoustic surveys, mid-water trawls and stable isotope analyses from scientific cruises conducted in 2009 and 2010. Mesopelagic micronekton performed diel vertical migrations in both provinces, from deep (400\textendash 740 m) to surface (0\textendash 200 m) layers at dusk and in the opposite direction at dawn, with some species migrating below 740 m. The EAFR province was more dynamic than the oligotrophic ISSG province, with enhanced eddy activity and enhanced yearly productivity. The active enrichment mechanisms in the EAFR, in terms of available primary production, led to high micronekton acoustic density (as a proxy of micronekton abundance) and large micronekton weight and abundance estimates from trawl data. Particulate organic matter in the EAFR exhibited greater enrichment in 13C and 15N compared to the ISSG and, consequently, tissues of selected micronekton organisms in the EAFR were more enriched in 15N (higher {$\delta$}15N values). In both provinces, micronekton encompassed a wide range of isotopic niches, with large overlaps between species. Micronekton and swordfish in the EAFR had an overlapping range of {$\delta$}15N values, contrasting with the ISSG province where swordfish were two trophic levels higher than the sampled micronekton. Our results provide some evidence that the combined action of riverine input and the dynamics of eddies might influence productivity in the EAFR, and hence the abundance of micronekton and the enrichment of tissues in 15N, compared to the oligotrophic ISSG province.},
  langid = {english},
  file = {C\:\\Users\\lloutr01\\Zotero\\storage\\6NEF2Z6Z\\Annasawmy et al. - 2018 - Micronekton diel migration, community composition .pdf}
}

