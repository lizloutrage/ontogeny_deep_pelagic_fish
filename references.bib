@article{papiolInfluenceEnvironmentalVariables2012,
  title = {Influence of Environmental Variables on the Spatio-Temporal Dynamics of Bentho-Pelagic Assemblages in the Middle Slope of the {{Balearic Basin}} ({{NW Mediterranean}})},
  author = {Papiol, V. and Cartes, J.E. and Fanelli, E. and Maynou, F.},
  year = {2012},
  month = mar,
  journal = {Deep Sea Research Part I: Oceanographic Research Papers},
  volume = {61},
