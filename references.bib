}

@article{butlerMesopelagicShesArabian2001,
  title = {Mesopelagic "shes of the {{Arabian Sea}}: Distribution, Abundance and Diet of {{Chauliodus}} Pammelas, {{Chauliodus}} Sloani, {{Stomias}} Aznis, and {{Stomias}} Nebulosus},
  author = {Butler, Mari and Bollens, Stephen M and Burkhalter, Brenda and Madin, Laurence P and Horgan, Erich},
  year = {2001},
  abstract = {Four species of predatory "shes \vphantom\{\} Chauliodus pammelas, Chauliodus sloani, Stomias aznis and Stomias nebulosus \vphantom\{\} were collected on two cruises to the Arabian Sea during 1995. We present data on the abundances, horizontal and vertical distributions, and diet of these "shes. We also discuss brie\#y the importance of the oxygen minimum zone and predation on myctophid "shes to the ecology of these mesopelagic predators. Chauliodus pammelas and C. sloani appear to have only partially overlapping horizontal distributions in the Arabian Sea, with C. pammelas more common to the north and C. sloani more common to the south. Our data support previous results suggesting that diel vertical migration is the norm for these species, with smaller individuals usually nearer to the surface and larger individuals tending to stay deeper. In contrast to Chauliodus, Stomias aznis and S. nebulosus appear to have largely overlapping horizontal distributions in the Arabian Sea. However, they may have slightly di!erent vertical distributions, with S. aznis living slightly shallower (especially at night) than S. nebulosus. All four species spend most of their time in the oxygen minimum zone, entering the surface oxygenated waters (100\vphantom\{\}150 m) only at night (if at all). The diets of C. pammelas, C. sloani, and S. aznis consisted mainly of lantern"shes, Myctophidae, and other "shes. In contrast, S. nebulosus, the smaller of the two Stomias species, ate mostly copepods and other crustaceans. This di!erential feeding may allow the two Stomias species to co-occur. Three of these four stomiids appear to play an important role in predation on myctophid "sh populations in the Arabian Sea. 2001 Elsevier Science Ltd. All rights reserved.},
  langid = {english},
  file = {C\:\\Users\\lloutr01\\Zotero\\storage\\HP4AV9AM\\Butler et al. - 2001 - Mesopelagic shes of the Arabian Sea distribution.pdf}
}

@article{butlerSwimbladderMorphologySpecific1972a,
  title = {Swimbladder {{Morphology}} and {{Specific Gravity}} of {{Myctophids}} off {{Oregon}}},
  author = {Butler, John L. and Pearcy, William G.},
  year = {1972},
  month = aug,
  journal = {Journal of the Fisheries Research Board of Canada},
  volume = {29},
  number = {8},
  pages = {1145--1150},
  issn = {0015-296X},
  doi = {10.1139/f72-170},
  abstract = {Three general types of swimbladders were found in the eight species of myctophids studied: gas-filled, fat-invested, and atrophied or reduced. Small specimens of all species had thin-walled, gas-filled swimbladders. Large specimens of Stenobrachius leucopsarus had fat-invested swim-bladders and large Diaphus theta had either gas-filled or atrophied swimbladders, as found by other workers. Large Tarletonbeania crenularis had either gas-filled or reduced swimbladders, large Lampanyctus ritteri and L. regalis had reduced swimbladders, and large Stenobrachius nannochir had fat-invested swimbladders. Protomyctophum thompsoni and P. crockeri retained gas-filled swimbladders.High body lipid content was found in S. leucopsarus, S. nannochir, L. ritteri and D. theta, and low lipid content was found in the other four species. Myctophids with high lipid content had specific gravities close to that of sea water (1.026\textendash 1.030). Tarletonbeania crenularis with a reduced swimbladder had a specific gravity of 1.088. Lampanyctus regalis had a lower specific gravity (1.040) due to high water content of the tissue.The swimbladder to body volumes in S. leucopsarus and D. theta were inversely related to body size and lipid content, indicating that lipids assume the primary buoyancy function as the gas-filled swimbladder regresses with age. This change may eliminate the physiological constraints imposed by a gas-filled swimbladder and permit the more extensive diel vertical migrations of adults.},
  langid = {english},
  file = {C\:\\Users\\lloutr01\\Zotero\\storage\\8X68KEFI\\Butler et Pearcy - 1972 - Swimbladder Morphology and Specific Gravity of Myc.pdf}
}

@article{caigerGrowthReproductionMesopelagic2021,
  title = {Growth and Reproduction in Mesopelagic Fishes: A Literature Synthesis},
