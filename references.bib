@article{astarloaIdentifyingMainInteractions2019,
  title = {Identifying Main Interactions in Marine Predator\textendash Prey Networks of the {{Bay}} of {{Biscay}}},
  author = {Astarloa, Amaia and Louzao, Maite and Boyra, Guillermo and Martinez, Udane and Rubio, Anna and Irigoien, Xabier and Hui, Francis K C and Chust, Guillem},
  editor = {{Travers-Trolet}, Morgane},
  year = {2019},
  month = dec,
  journal = {ICES Journal of Marine Science},
  volume = {76},
  number = {7},
  pages = {2247--2259},
  issn = {1054-3139, 1095-9289},
  doi = {10.1093/icesjms/fsz140},
  abstract = {Identifying the role that environmental factors and biotic interactions play in species distribution can be essential to better understand and predict how ecosystems will respond to changing environmental conditions. This study aimed at disentangling the assemblage of the pelagic predator\textendash prey community by identifying interspecific associations and their main drivers. For this purpose, we applied the joint species distribution modelling approach, JSDM, to the co-occurrence patterns of both prey and top predator communities obtained from JUVENA surveys during 2013\textendash 2016 in the Bay of Biscay. Results showed that the co-occurrence patterns of top predators and prey were driven by a combination of environmental and biotic factors, which highlighted the importance of considering both components to fully understand the community structure. In addition, results also revealed that many biotic interactions, such as schooling in prey (e.g. anchovy\textendash sardine), local enhancement/facilitation in predators (e.g. Cory's shearwater\textendash fin whale), and predation between predator\textendash prey species (e.g. northern gannet\textendash horse mackerel), were led by positive associations, although predator avoidance behaviour was also suggested between negatively associated species (e.g. striped dolphin\textendash blue whiting). The identification of interspecific associations can therefore provide insights on the functioning of predators\textendash prey network and help advance towards an ecosystem-based management.},
  langid = {english},
  file = {C\:\\Users\\lloutr01\\Zotero\\storage\\YS5II5B4\\Astarloa et al. - 2019 - Identifying main interactions in marine predator–p.pdf}
}

