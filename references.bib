@article{hulleyUpperslopeDistributionsOceanic1992,
  title = {Upper-Slope Distributions of Oceanic Lanternfishes (Family: {{Myctophidae}})},
  shorttitle = {Upper-Slope Distributions of Oceanic Lanternfishes (Family},
  author = {Hulley, P. Alexander},
  year = {1992},
  month = nov,
  journal = {Marine Biology},
  volume = {114},
  number = {3},
