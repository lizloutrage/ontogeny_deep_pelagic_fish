@article{fockSizeStructureChanges2019,
  title = {Size Structure Changes of Mesopelagic Fishes and Community Biomass Size Spectra along a Transect from the Equator to the {{Bay}} of {{Biscay}} Collected in 1966\textendash 1979 and 2014\textendash 2015},
