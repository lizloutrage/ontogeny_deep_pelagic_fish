@article{portnerClimateChangeAffects2007,
  title = {Climate {{Change Affects Marine Fishes Through}} the {{Oxygen Limitation}} of {{Thermal Tolerance}}},
  author = {P{\"o}rtner, Hans O. and Knust, Rainer},
