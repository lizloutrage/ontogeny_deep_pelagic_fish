@article{kaartvedtEnlighteningOceanTwilight2019,
  title = {Enlightening the Ocean's Twilight Zone},
  author = {Kaartvedt, Stein and Langbehn, Tom J and Aksnes, Dag L},
  editor = {Browman, Howard},
  year = {2019},
