@article{contrerasFeedingEcologyEarly2019a,
  title = {Feeding Ecology of Early Life Stages of Mesopelagic Fishes in the Equatorial and Tropical {{Atlantic}}},
  author = {Contreras, Tabit and Olivar, M Pilar and Hulley, P Alexander and {Fern{\'a}ndez de Puelles}, M Luz},
