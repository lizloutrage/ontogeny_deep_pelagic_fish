@article{fariasOntogeneticSpatialDynamics2022,
  title = {Ontogenetic Spatial Dynamics of the Deep-Sea Teleost {{Aphanopus}} Carbo in the {{NE Atlantic}} According to Otolith Geochemistry},
  author = {Farias, In{\^e}s and {P{\'e}rez-Mayol}, S{\'i}lvia and Vieira, Sofia and Oliveira, Paulo B. and Figueiredo, Ivone and {Morales-Nin}, Beatriz},
